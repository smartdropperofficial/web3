{
  "version": 2,
  "builds": [
       {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"], "maxDuration": 300000 }
       }
  ],
  "routes": [
       {
            "src": "/(.*)",
            "dest": "dist/index.js"
       }
  ]
}
